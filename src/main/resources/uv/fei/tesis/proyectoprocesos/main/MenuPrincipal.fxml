<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import java.net.URL?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #7900FF;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uv.fei.tesis.proyectoprocesos.main.MenuPrincipal">
   <children>
      <VBox alignment="CENTER" layoutX="123.0" layoutY="121.0" prefHeight="159.0" prefWidth="355.0" style="-fx-background-color: #548CFF; -fx-background-radius: 20;">
         <children>
            <Button fx:id="btnRegistrarProyecto" mnemonicParsing="false" onAction="#clicRegistrarProyecto" prefHeight="53.0" prefWidth="262.0" style="-fx-border-radius: 20;" text="Registrar proyecto">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="btnDetallesDeProyecto" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#clicDetallesDeProyecto" prefHeight="51.0" prefWidth="261.0" text="Detalles proyecto">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="btnreporteMensualProyecto" layoutX="10.0" layoutY="36.0" mnemonicParsing="false" onAction="#clicReporteMensualProyecto" prefHeight="50.0" prefWidth="263.0" text="Reporte mensual proyecto">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@style/Style.css"/>
   </stylesheets>
</AnchorPane>
