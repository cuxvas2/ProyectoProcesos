<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import java.net.URL?>
<VBox alignment="CENTER" prefHeight="468.0" prefWidth="756.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="uv.fei.tesis.proyectoprocesos.main.RegistrarProyectoController">
   <children>
      <HBox alignment="CENTER" prefHeight="71.0" prefWidth="227.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="71.0" prefWidth="361.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Nombre del proyecto:">
                     <HBox.margin>
                        <Insets left="20.0" right="5.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txtNombreDeProyecto" prefHeight="25.0" prefWidth="211.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="71.0" prefWidth="365.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Tipo de proyecto:" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Label>
                  <ChoiceBox fx:id="boxTipoProyecto" prefHeight="25.0" prefWidth="256.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </ChoiceBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="69.0" prefWidth="227.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="69.0" prefWidth="361.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Nombre del presentador:">
                     <HBox.margin>
                        <Insets left="20.0" right="5.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txtNombrePresentador" prefHeight="25.0" prefWidth="195.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="69.0" prefWidth="363.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Carrera:">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Label>
                  <ChoiceBox fx:id="boxCarrera" prefHeight="25.0" prefWidth="306.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </ChoiceBox>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="67.0" prefWidth="424.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Fecha en que se tituló:">
                     <HBox.margin>
                        <Insets left="20.0" right="5.0" />
                     </HBox.margin>
                  </Label>
                  <DatePicker fx:id="dateFechaTitulacion" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </DatePicker>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Director:">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txtNombreDirector" prefHeight="25.0" prefWidth="236.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="51.0" prefWidth="756.0" VBox.vgrow="ALWAYS">
         <children>
            <Label text="Sinodailes:">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin></Label>
            <TextField fx:id="txtSinodailes" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="154.0" prefWidth="756.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="201.0" prefWidth="755.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Descripción del tema:">
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </Label>
                  <TextArea fx:id="txtDescripcionTema" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="5.0" left="20.0" right="20.0" />
                     </VBox.margin>
                  </TextArea>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="756.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnRegistrar" mnemonicParsing="false" onAction="#accionRegistrar" text="Registrar" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnRegresar" mnemonicParsing="false" onAction="#accionRegresar" text="Regresar" />
               </children>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </HBox>
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@style/Style.css"/>
   </stylesheets>
</VBox>
